import java.util.*;
import java.util.Scanner;
class Main{
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int arr[]=new int[a];
        for(int i=0;i<a;i++){
            arr[i]=sc.nextInt();
        }
        int b=arr[1];
        for(int i=0;i<a;i++){
            if(b<arr[i]){
                b=arr[i];
            }
        }
        System.out.println("Largest element:"+b);
    }
}
